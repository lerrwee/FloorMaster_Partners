<Window x:Class="FloorMaster_Partners.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FloorMaster_Partners"
        mc:Ignorable="d"
        Icon="/Resources/Мастер пол.ico"
        Height="450" Width="800"
        MinHeight="400" MinWidth="800"
        
        Title="Мастер пол | Партнеры">

    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="2*"></RowDefinition>
            <RowDefinition Height="10*"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>

        <Grid Background="#FF67BA80" Grid.Row="0" >
            <Image Source="/Resources/Мастер пол.ico" Margin="30,10,10,10" VerticalAlignment="Center" HorizontalAlignment="Left" />
            <Label Content="Мастер пол" FontSize="20" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center"/>

        </Grid>
        <Grid Background="White" Grid.Row="1" >
            <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" />
        </Grid>
        <Grid Background="#F4E8D3 " Grid.Row="2" >


        </Grid>
    </Grid>
</Window>
